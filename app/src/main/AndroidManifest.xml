<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.na6ix.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuizActivity"
            android:label="Geography" android:screenOrientation="portrait"/>
        <activity android:name=".QuizActivity2"
            android:label="History" android:screenOrientation="portrait"/>
        <activity android:name=".QuizActivity3"
            android:label="Science" android:screenOrientation="portrait"/>
        <activity android:name=".QuizActivity4"
            android:label="Art" android:screenOrientation="portrait"></activity>
    </application>

</manifest>